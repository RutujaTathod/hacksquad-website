#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    while(n--){
        int x;
        cin>>x;
        if(x%3==0){
            cout<<1*(x/3)<<" "<<(x/3)<<endl;
        }else{
            if(x%3==1){
                cout<<1*(x/3)+1<<" "<<(x/3)<<endl;
            }else{
                if(x%3==2){
                    cout<<1*(x/3)<<" "<<(x/3)+1<<endl;
                }
            }
        }

    }
}
